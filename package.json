{
  "name": "multiple-main",
  "version": "0.1.0",
  "private": true,
  "description": "多仓库管理（git submodule+Nx）前端项目",
  "scripts": {
    "start": "nx run-many --target=serve --all",
    "build": "nx run-many --target=build --all"
  },
  "devDependencies": {
    "nx": "20.4.6",
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "latest"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "pnpm@9.1.0"
}